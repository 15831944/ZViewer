IDR_MAIN_MENU MENU 
BEGIN
    POPUP "파일(&F)"
    BEGIN
        MENUITEM "파일 열기(&O)",               ID_MAINMENU_FILE_OPEN
        MENUITEM SEPARATOR
        MENUITEM "종료(&X)",                    ID_MAINMENU_FILE_EXIT
    END
    POPUP "보기(&V)"
    BEGIN
        MENUITEM "전체화면(&F)",                ID_VIEW_FULLSCREEN
        MENUITEM "오른쪽 위부터 보기",          ID_VIEW_RIGHTTOPFIRSTDRAW
        MENUITEM "큰 그림 화면에 맞게 축소",    ID_VIEW_BIGTOSCREENSTRETCH
        MENUITEM "작은 그림 화면에 맞게 확대",  ID_VIEW_SMALLTOSCREENSTRETCH
    END
    POPUP "이동(&M)"
    BEGIN
        MENUITEM "다음 이미지(&N)\tPageDown",   ID_MOVE_NEXTIMAGE
        MENUITEM "이전 이미지(&P)\tPageUp",     ID_MOVE_PREVIMAGE
        MENUITEM SEPARATOR
        MENUITEM "처음 이미지(&F)\tHome",       ID_MOVE_FIRSTIMAGE
        MENUITEM "마지막 이미지(&L)\tEnd",      ID_MOVE_LASTIMAGE
        MENUITEM SEPARATOR
        MENUITEM "이미지 위치 되돌림\tBackspace", ID_MOVE_UNDOIMAGEPOSITION
    END
    POPUP "옵션(&O)"
    BEGIN
        MENUITEM "파일 확장자...",              ID_OPTION_FILE_EXT
    END
    POPUP "도움말(&H)"
    BEGIN
        MENUITEM "정보(&A)",                    ID_HELP_ABOUT
    END
END

IDR_POPUP_MENU MENU 
BEGIN
    POPUP "PopupMenu"
    BEGIN
        MENUITEM "처음 이미지",                 ID_MOVE_FIRSTIMAGE
        MENUITEM "마지막 이미지",               ID_MOVE_LASTIMAGE
        MENUITEM SEPARATOR
        MENUITEM "전체화면",                    ID_VIEW_FULLSCREEN
        MENUITEM "오른쪽 위부터 그리기",        ID_VIEW_RIGHTTOPFIRSTDRAW
        MENUITEM SEPARATOR
        MENUITEM "큰 그림 화면에 맞게 축소",    ID_POPUPMENU_BIGTOSCREENSTRETCH

        MENUITEM "작은 그림 화면에 맞게 확대",  ID_POPUPMENU_SMALLTOSCREENSTRETCH
        , GRAYED
        MENUITEM SEPARATOR
        MENUITEM "현재 파일 지우기\tDelete",    ID_DELETETHISFILE
        MENUITEM SEPARATOR
        MENUITEM "종료(&X)",                    ID_MAINMENU_FILE_EXIT
    END
END


